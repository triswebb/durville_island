	journal = {Palaeogeography, Palaeoclimatology, Palaeoecology},
	keywords = {Charcoal analysis,Fire history,Mineral magnetism,Pollen analysis,Southern Alps},
	mendeley-groups = {MSc Thesis/magneitc properties},
