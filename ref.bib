}
@article{Williams2010,
	abstract = {All aspects of Quaternary vegetation distributions, from the range and abundance of individual species, to $\backslash$nthe global position, extent, and composition of major vegetation formations, have changed extensively and $\backslash$nrepeatedly in response to the orbitally paced oscillations in climate, atmospheric CO2, and ice extent that $\backslash$nare characteristic of the Quaternary.   At centennial and longer timescales, the distribution and abundance $\backslash$nof plant species closely track shifts in climate, despite the rooted habit of individual plants, via the dispersal $\backslash$nof  propagules  to  newly  favorable  regions,  local  changes  in  population  abundances,  and  the  dying  out  of $\backslash$npopulations in unfavorable locations.  Distributions of vegetation formations and biomes emerge from these $\backslash$nspecies-level responses.  The history of Quaternary vegetation distributions is thus indirectly a history of $\backslash$nglobal and regional climate change, and paleovegetational records are an important source of information $\backslash$nabout  Quaternary  environments.    As  an  active  component  of  the  earth  system,  shifts  in  the  global $\backslash$nvegetation affect the biogeochemical and biogeophysical fluxes between the land surface and atmosphere $\backslash$nas  well  as  the  functioning  of  the  global  carbon  cycle.    Moreover,  the  present  genetic  structure  and $\backslash$ndistribution  of  populations,  species,  and  communities  is  in  part  a  legacy  of  late-Quaternary  species $\backslash$nmigrations and colonization history.},
	author = {Williams, J.W.},
	doi = {10.1126/science.206.4418.550},
	file = {:Users/tristanwebb/Desktop/MSc/papers in nz/Quaternary Vegetation Distributions.pdf:pdf},
	issn = {0036-8075},
	journal = {Encyclopedia of Paleoclimatology and Ancient Environments (in press)},
	mendeley-groups = {MSc Thesis/NZ},
	pages = {1--15},
	title = {{Quaternary Vegetation Distributions}},
	url = {http://www.geography.wisc.edu/faculty/williams/lab/pubs/WilliamsKluwerEncycV4final.pdf},
	volume = {2523},
	year = {2010}
}

@article{Burney1994,
	abstract = {An 8 n{\~{}} sediment core from Laguna Tortuguero, Puerto Rico, provides a 7000 calendar year history of fire occurrence and sedimentation on the island's north coast. After c. 5300 cal-BP, n{\~{}}icroscopic charcoal particle concentration and influx increase abruptly, and values remain high for the next two millennia. Subsequent to c. 3200 cal-BP, fire occurrence gradually declines to more moderate levels. It seems likely that the mid-Holocene acceleration in fire frequency documented here may signal the onset of human disturbance of the landscape. Indirect detection of human arrival on oceanic islands from a sudden increase in stratigraphic charcoal may be a useful technique in other island contexts, although it is important to consider that wildfires can occur without a human ignition source.},
	author = {Burney, D. A. and Burney, L. P. and MacPhee, R. D. E.},
	journal = {Journal of Archaeological Science},
	doi = {10.1006/jasc.1994.1027},
	file = {:Users/tristanwebb/Desktop/MSc/papers in nz/Holocene Charcoal Stratigraphy from Laguna Tortuguero, Puerto Rico, and the Timing of Human Arrival on the Island.pdf:pdf},
	isbn = {0305-4403},
	issn = {0305-4403},
	keywords = {charcoal stratigraphy,fire history,human arrival,landscape modification,oceamc islands,puerto rico},
	pages = {273--281},
	title = {{Holocene charcoal stratigraphy from Laguna Tortuguero, Puerto Rico, and the timing of human arrival on the Island}},
	url = {http://www.sciencedirect.com/science/article/pii/S0305440384710272},
	volume = {21},
