	journal = {Canadian Journal of Forest Research},
	mendeley-groups = {MSc Thesis/magneitc properties},
	number = {5},
	pages = {774--787},
	pmid = {70},
	title = {{A 9000-year fire history from the Oregon Coast Range, based on a high-resolution charcoal study}},
	volume = {28},
	year = {1998}
}
@article{Millspaugh1995,
	abstract = {A 750-year fire history was reconstructed for the Central Plateau of$\backslash$nYellowstone National Park from the deep-water sediments of five lakes.$\backslash$nThe charcoal record from a large lake provided a chronology of regional$\backslash$nfires. Data from four small lakes were used to study local and$\backslash$nextralocal fires. The co-occurrence of abundant charcoal and high$\backslash$nmagnetic-susceptibility values at the same stratigraphic level was used$\backslash$nas evidence of a local catchment fire, and a charcoal peak without high$\backslash$nmagnetic susceptibility was ascribed to an extralocal fire or a local$\backslash$nfire without a related erosion event. The fire history was compared with$\backslash$nthe dendrochronologic fire record for the last 450 years, and the close$\backslash$nagreement provided the justification to extend the chronology back in$\backslash$ntime. Large areas of the region burned in AD 1988, c. 1700, c. 1560, and$\backslash$nc. 1440. From c. 1220 to 1440 and c. 1700 to 1987, intermediate to small$\backslash$nareas burned. The near-absence of fires in the twentieth century prior$\backslash$nto the large fires of 1988 is evident in the charcoal record.},
